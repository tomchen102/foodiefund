{
  "extends": [
    "next",
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": ["@typescript-eslint", "prettier", "import"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": true,
        "singleQuote": false,
        "printWidth": 80,
        "tabWidth": 2,
        "trailingComma": "es5",
        "endOfLine": "auto"
      }
    ]
  }
}
